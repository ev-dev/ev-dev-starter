{
  "name": "ev-dev-starter",
  "version": "0.2.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build-dev & npm run serve-dev",
    "build-dev": "NODE_ENV=development webpack -w",
    "serve-dev": "NODE_ENV=development nodemon -r @std/esm src/server --watch src/server",
    "test-serve": "NODE_ENV=development nodemon -r @std/esm src/server/tests",
    "test-build": "NODE_ENV=development webpack --bail --progress -d",
    "start": "npm run build && npm run serve",
    "build": "NODE_ENV=production webpack",
    "serve": "NODE_ENV=production node src/server"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "ev-dev",
  "license": "MIT",
  "@std/esm": {
    "esm": "all",
    "cjs": {
      "interop": true,
      "namedExports": true,
      "paths": true,
      "vars": true
    },
    "debug": true,
    "sourceMap": true,
    "warnings": false
  },
  "dependencies": {
    "@std/esm": "0.18.0",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "chalk": "^2.3.0",
    "css-loader": "^0.28.7",
    "node-sass": "^4.7.2",
    "nodemon": "^1.13.2",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "volleyball": "^1.4.1",
    "webpack": "^3.10.0",
    "webpack-livereload-plugin": "^1.0.0"
  }
}
